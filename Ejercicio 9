#include <stdio.h>

int main() {
    int n, i;

    // Número de la tabla
    printf("Ingrese un número entero del cual quiere su tabla de multiplicar: ");
    scanf("%d", &n);

    // Tabla del número
    printf("Tabla de multiplicar del %d:\n", n);
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", n, i, n * i);
    }

    return 0;
}
