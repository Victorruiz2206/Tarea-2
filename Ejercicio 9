#include <stdio.h>

int main() {
    int N;
    
    // Solicitar al el número
    printf("Introduce un número entero para obtener su tabla de multiplicar: ");
    scanf("%d", &N);
    
    // Mostrar la tabla de multiplicar del número
    printf("Tabla de multiplicar del %d:\n", N);
    for(int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", N, i, N * i);
    }

    return 0;
}

